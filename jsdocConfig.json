{
    "tags": {
        "allowUnknownTags": true
    },
    "opts": {
        "template": "node_modules/docdash",
        "destination": "./doc",
        "private": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "theme": "cerulean",
        "navType": "vertical",
        "linenums": true,
        "dateFormat": "MMMM Do YYYY, h:mm:ss a"
    }
}
